<launch>
<!-- 启动两对乌龟 GUI 与键盘控制节点 -->
    <group ns = "first">
        <node name="my_turtle1" pkg="turtlesim" type="turtlesim_node" output="screen"/>
        <node name="my_key1" pkg="turtlesim" type="turtle_teleop_key" output="screen"/>           
    </group>

    <group ns = "second">
        <node name="my_turtle2" pkg="turtlesim" type="turtlesim_node" output="screen"/>
        <node name="my_key2" pkg="turtlesim" type="turtle_teleop_key" output="screen"/>        
    </group>
</launch>