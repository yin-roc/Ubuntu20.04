<!-- 6、launch文件标签之 rosparam -->
<launch>
    <!-- (1)rosparam 格式1：launch下，node外 -->
    <!-- 加载参数 -->
    <rosparam command="load" file="$(find nine_launch01_basic)/launch/params.yaml"/>
    
    <node name="helloworld" pkg="six_plumbing_head" type="hello" output="screen"/>    
    <node name="my_turtle" pkg="turtlesim" type="turtlesim_node" >
    <!-- (2)rosparam 格式2：node下 -->
        <rosparam command="load" file="$(find nine_launch01_basic)/launch/params.yaml"/>
    </node>
    <node name="my_key" pkg="turtlesim" type="turtle_teleop_key" />
</launch>